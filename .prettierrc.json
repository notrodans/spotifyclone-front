{
  "trailingComma": "none",
  "semi": false,
  "useTabs": true,
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "bracketSameLine": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "printWidth": 130,
  "importOrder": ["^../(.*)", "^./(.*)"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
